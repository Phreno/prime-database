#
# Récupère le nième prime à partir de 0

primeDB_GET_NTH_SH="${BASH_SOURCE[0]:-$(realpath ${0})}"
primeDB_GET_NTH_SH_DIR="$( dirname ${primeDB_GET_NTH_SH} )"

#
# Gestion des erreurs
#
source ${primeDB_GET_NTH_SH_DIR}/lib/error.profile

#
# Import des dépendances
#
source ${primeDB_DEBUG_CORE_SH_DIR}/lib/base.profile
source ${primeDB_DEBUG_CORE_SH_DIR}/lib/core.profile

USAGE="USAGE: $( basename ${0} ) index"

getNth(){
  nth=${1?${USAGE}}
  i=1
  while [ i -lt primeDB_CHUNK_LINES ]; do
  done
}

getNth ${1}
